abstract class Evaluation {
    protected int id;
    protected String name;
    protected double weight;

    public Evaluation(int id, String name, double weight) {
        this.id = id;
        this.name = name;
        this.weight = weight;
    }

    public int getId() {
        return id;
    }

    public String getName() {
        return name;
    }

    public double getWeight() {
        return weight;
    }

    public abstract double getScore();
    @Override
    public String toString() {
        return "Evaluation{" + "id=" + id + ", name='" + name +   ", weight=" + weight;
    }

}
